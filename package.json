{
  "name": "ascii-diagram-drawer",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "scripts": {
    "gen:proto": "npx protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/gen/ --ts_proto_opt=esModuleInterop=true,forceLong=string -I ./shared/messages ./shared/messages/*.proto",
    "dev": "npm run gen:proto && vite dev",
    "build": "npm run gen:proto && vite build",
    "preview": "npm run gen:proto && vite preview",
    "check": "npm run gen:proto && svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "npm run gen:proto && svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "tauri": "npm run gen:proto && tauri"
  },
  "license": "MIT",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "bootstrap": "^5.3.6"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.9.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tauri-apps/cli": "^2",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "ts-proto": "^2.7.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.3"
  }
}
